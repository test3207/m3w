{
  "name": "@m3w/shared",
  "version": "2.0.4",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./schemas": {
      "types": "./dist/schemas.d.ts",
      "import": "./dist/schemas.js"
    },
    "./api-contracts": {
      "types": "./dist/api-contracts.d.ts",
      "import": "./dist/api-contracts.js"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "import": "./dist/constants.js"
    },
    "./transformers": {
      "types": "./dist/transformers.d.ts",
      "import": "./dist/transformers.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch"
  },
  "dependencies": {
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
